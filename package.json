{
  "name": "beatflow",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://aiempirehq.github.io/beatflow",
  "dependencies": {
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@msgpack/msgpack": "^2.8.0",
    "@react-three/fiber": "^8.13.0",
    "@react-three/postprocessing": "^2.15.0",
    "@wasm-tool/wasm-pack-plugin": "^1.7.0",
    "comlink": "^4.4.1",
    "framer-motion": "^10.12.0",
    "onnxruntime-web": "^1.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.0",
    "react-intersection-observer": "^9.5.0",
    "react-router-dom": "^6.14.0",
    "react-swipeable": "^7.0.1",
    "reconnecting-websocket": "^4.4.0",
    "three": "^0.154.0",
    "tone": "^14.7.77"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/plugin-proposal-class-properties": "^7.18.0",
    "@babel/plugin-transform-runtime": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "babel-loader": "^9.1.0",
    "compression-webpack-plugin": "^10.0.0",
    "core-js": "^3.31.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "lighthouse": "^10.0.0",
    "style-loader": "^3.3.0",
    "svgo": "^3.0.2",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.88.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0",
    "worker-loader": "^3.0.8",
    "worker-plugin": "^5.0.1"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.config.js",
    "analyze": "cross-env ANALYZE=true npm run build",
    "test": "jest",
    "test:performance": "lighthouse http://localhost:3000 --output json --output html --output-path ./lighthouse-report.html",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "wasm:build": "wasm-pack build wasm --target web",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "postinstall": "npm run wasm:build",
    "lighthouse": "lighthouse http://localhost:3000 --quiet --chrome-flags=\"--headless\" --output-path=./lighthouse-report.html"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  }
} 